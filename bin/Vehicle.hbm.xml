<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernatetest.POJO">
	<class name="Vehicle" table="Vehicle">
		<id name="Vehicle_ID" type="integer" column="vehicle_id">
			<generator class="increment"></generator>
		</id>
		<property name="OwnerName" type="text" column="owner_name"></property>
		<property name="RegistrationNumber" type="text" column="registration_number"></property>
		<property name="VehicleLaunchDate" type="date" column="vehicle_launch_date"></property>
	</class>
</hibernate-mapping>